<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" strict="true">
    <Appenders>
		<Appender type="Socket" name="LOG" host="logger" port="9987">
			<JsonLayout properties="true" complete="true" compact="true"/>
		</Appender>
	</Appenders>
	<Loggers>
    		<Root level="ALL">
    			<AppenderRef ref="LOG"/>
    		</Root>
    </Loggers>
</Configuration>
